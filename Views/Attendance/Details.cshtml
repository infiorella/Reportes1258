@model Models.Asistencia

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Details</title>
</head>
<body>

<div>
    <h4>Asistencia</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Fecha de Asistencia
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.FechaAsistencia)
        </dd>
        <dt class = "col-sm-2">
            Estado
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Estado)
        </dd>
        <dt class = "col-sm-2">
            Fecha de Creacion
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.FechaCreacion)
        </dd>
        <dt class = "col-sm-2">
            Usuario que lo créo
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.UserCreacion)
        </dd>
        <dt class = "col-sm-2">
            Alumno
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdAlumnoNavigation.IdAlumno)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.IdAsistencia">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
</body>
</html>
