@model IEnumerable<Nota>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.AreaCurricular)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Competencia)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Cal1)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Cal2)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Cal3)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CalAnual)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Comentario)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdEstado)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FechaCreacion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UserCreacion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdAlumnoNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.AreaCurricular)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Competencia)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Cal1)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Cal2)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Cal3)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.CalAnual)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Comentario)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.IdEstado)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.FechaCreacion)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.UserCreacion)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.IdAlumnoNavigation.IdAlumno)
                </td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.IdNota">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.IdNota">Details</a> |
                    <a asp-action="Delete" asp-route-id="@item.IdNota">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
